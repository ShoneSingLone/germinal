{
  "name": "germinal",
  "version": "0.0.0",
  "scripts": {
    "bs": "npm i --legacy-peer-deps && npm run make:language",
    "start": "cross-env NODE_ENV=development vite --host",
    "lint": "npm run lint:src && npm run lint:yapi",
    "lint:src": "prettier --write ./src",
    "lint:yapi": "prettier --write ./src_yapi",
    "serve": "vite preview",
    "build": "npm run build:ui && npm run build:app && npm run build:pwa",
    "build:app": "cross-env NODE_ENV=production cross-env type=app vite build",
    "build:ui": "cross-env NODE_ENV=production cross-env type=lib vite build && npm run copy:ui",
    "build:pwa": "node ./build/make/PWA/maker.js",
    "copy:ui": "node ./build/tools/copy_ui.js",
    "preview": "cross-env NODE_ENV=development vite --host --open",
    "git:pull": "git pull && git pull gitee master ",
    "git:push": "git push && git push gitee master",
    "make:itemRender": "node ./build/make.js itemRender",
    "make:language": "node ./build/make.js language"
  },
  "main": "lsrc/main.js",
  "dependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "@popperjs/core": "^2.10.2",
    "@surely-vue/table": "^2.4.7",
    "ant-design-vue": "^3.0.0-alpha.3",
    "axios": "^0.26.0",
    "dayjs": "^1.10.7",
    "germinal_api": "file:./local_modules/api",
    "idb-keyval": "^6.2.0",
    "immer": "^9.0.15",
    "jquery": "^3.6.0",
    "json5": "^2.2.1",
    "jsondiffpatch": "^0.4.1",
    "lodash": "^4.17.21",
    "lsrc": "file:./src/",
    "md5": "^2.3.0",
    "nprogress": "^0.2.0",
    "sass": "^1.52.2",
    "vue": "^3.2.16",
    "vue-cli": "^2.9.6",
    "vue-router": "^4.0.11",
    "vue3-virtual-scroller": "^0.2.3",
    "ysrc": "file:./src_yapi/"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@ventose/utils-node": "^0.0.11",
    "@vitejs/plugin-basic-ssl": "^0.1.2",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vitejs/plugin-vue-jsx": "^1.2.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-plugin-transform-commonjs": "^1.1.6",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.19.1",
    "jimp": "^0.16.1",
    "less": "^4.1.2",
    "lint-staged": "^9.5.0",
    "mockjs": "^1.1.0",
    "prettier": "^2.2.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-external-globals": "^0.6.1",
    "svgo": "^2.8.0",
    "vite": "^2.6.4",
    "vite-plugin-html": "^2.1.2",
    "vite-plugin-importer": "^0.2.5",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,vue}": [
      "prettier --write ./src --ignore-unknown",
      "prettier --write ./local_modules --ignore-unknown",
      "git add"
    ]
  },
  "types": "types.d.ts"
}
